{
  "expo": {
    "name": "Zeger Customer",
    "slug": "zeger-customer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "zeger-customer",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#EA2831"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.zeger.customer",
      "config": {
        "googleMapsApiKey": "AIzaSyBX-Ud57WsonDCCPfx-X6KsStupHdcRlNc"
      },
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Zeger membutuhkan akses lokasi untuk menemukan rider terdekat dan melacak pengiriman pesanan Anda.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Zeger membutuhkan akses lokasi untuk menemukan rider terdekat dan melacak pengiriman pesanan Anda."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#EA2831"
      },
      "package": "com.zeger.customer",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBX-Ud57WsonDCCPfx-X6KsStupHdcRlNc"
        }
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "name": "Zeger Customer",
      "shortName": "Zeger Customer",
      "lang": "id",
      "scope": "/",
      "themeColor": "#EA2831",
      "backgroundColor": "#EA2831",
      "display": "standalone",
      "orientation": "portrait",
      "startUrl": "/",
      "preferRelatedApplications": false,
      "dangerouslyAllowInsecureHttpInExpoGo": false
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Zeger membutuhkan akses lokasi untuk menemukan rider terdekat."
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "kotlinVersion": "1.9.25",
            "suppressKotlinVersionCompatibilityCheck": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "4ec080a9-ff0c-45ea-b054-a49952e5a6a5"
      }
    },
    "owner": "koncomyid"
  }
}
