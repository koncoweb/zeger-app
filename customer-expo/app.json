{
  "expo": {
    "name": "Zeger Customer",
    "slug": "zeger-customer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "zeger-customer",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#EA2831"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.zeger.customer",
      "config": {
        "googleMapsApiKey": "AIzaSyBX-Ud57WsonDCCPfx-X6KsStupHdcRlNc"
      },
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Zeger membutuhkan akses lokasi untuk menemukan rider terdekat dan melacak pengiriman pesanan Anda.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Zeger membutuhkan akses lokasi untuk menemukan rider terdekat dan melacak pengiriman pesanan Anda."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#EA2831"
      },
      "package": "com.zeger.customer",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBX-Ud57WsonDCCPfx-X6KsStupHdcRlNc"
        }
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "buildType": "apk"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Zeger membutuhkan akses lokasi untuk menemukan rider terdekat."
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "kotlinVersion": "1.9.25",
            "suppressKotlinVersionCompatibilityCheck": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0d33273b-3bb5-4873-9cd8-b763f66f6efe"
      }
    },
    "owner": "koncoweb"
  }
}
